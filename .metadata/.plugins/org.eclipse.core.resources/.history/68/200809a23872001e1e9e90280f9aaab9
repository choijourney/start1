package chap3;

// 대입 연산자 : =,+=,-=,*=,..

public class OpEx7 {

	public static void main(String[] args) {
		int num = 100;
		System.out.println(num);
		num += 10;
		System.out.println(num);
		num -= 10;
		System.out.println(num);
		num *= 5;
		System.out.println(num);
		num /= 3;
		System.out.println(num);
		num %= 2 + 3;
		System.out.println(num);
	}

}
