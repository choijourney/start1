package chap3;

// 이항연산자 : 산술연산자 
// + 
// - 
// *
// /
// %

public class OpEx3 {

	public static void main(String[] args) {
		int x = 10, y = 8;
		System.out.printf("%d+%d=%d\n", x, y, (x + y)); // x+y=18
		System.out.printf("%d-%d=%d\n", x, y, (x - y));
		System.out.printf("%d*%d=%d\n", x, y, (x * y));
		System.out.printf("%d/%d=%d\n", x, y, (x / y));
		System.out.printf("%d%%d=%d\n", x, y, (x % y));

	}

}
