package chap3;

// 단항 연산자 : 논리부정 연산자

// 		! :
// 단항 연산자 : 부호 연산자
//    	+ :

public class OpEx2 {

	public static void main(String[] args) {

		System.out.println("true=" + !false);
		System.out.println("false=" + !true);
		System.out.println("5!=4 :" + !(5 != 4));
		System.out.println("5==4 :" + !(5 == 4));

	}

}
