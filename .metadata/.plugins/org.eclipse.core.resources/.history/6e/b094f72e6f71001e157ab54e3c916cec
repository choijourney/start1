package chap2;

public class varEx1 {

	public static void main(String[] args) {

		int num; // 변수의 선언
		num = 100; // 변수의 초기화
		// num 변수를 콘솔 출력하기
		System.out.println(num);  //100  // Syso ctrl+space
		num = 200;	// 값을 변경
		System.out.println(num); //200

		int num2 = 100; // 선언과 초기화 동시에.
		num2= 200;
		System.out.println(num2);
	}

}
