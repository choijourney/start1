package chap3;

// 이항연산자 : 비교연산자 => 결과는 boolean 

public class OpEx4 {

	public static void main(String[] args) {
		int x = 1, y = 1;
		System.out.printf("%d == %d : %b\n", x, y, (x == y));
		System.out.printf("%d == %d : %b\n", x, y, (x != y));
		System.out.printf("%d == %d : %b\n", x, y, (x > y));
		System.out.printf("%d == %d : %b\n", x, y, (x >= y));

	}

}
