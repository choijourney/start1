package chap2;

// 다른 자료형 사이의 연산의 결과 자료형
// 큰자료형  <=  큰 자료형 + 작은 자료형

public class varEx5 {

	public static void main(String[] args) {
		int i1 = 100;
		int i2 = 200;
		int i3 = i1+i2;
		float f1 = 10.0f;
		float f2 = i3+ f1;  // 310.0
		double d1 = f1+10.5; // 20.5
		
		System.out.println("i3="+ i3);
System.out.println("f2=" + f2);
System.out.println("d1="+d1);
	}

}
