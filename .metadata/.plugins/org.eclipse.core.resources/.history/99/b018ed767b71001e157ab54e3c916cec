package chap2;

// 형변환 예제 1

public class varEx4 {

	public static void main(String[] args) {
		byte b = 10;
		short s = 20;
		int i = 30;
		int l =40;
		float f = 50;
		double d = 60;
		System.out.println(b);
		System.out.println(s);
		System.out.println(i);
		System.out.println(f);
		System.out.println(d);

		d = b;
	}

}
